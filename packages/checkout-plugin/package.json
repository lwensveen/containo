{
  "name": "@containo/checkout-plugin",
  "version": "0.0.0",
  "devDependencies": {
    "esbuild": "^0.25.9"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "npm run build:types && npm run build:esm && npm run build:umd",
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=dist/index.esm.js",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:umd": "esbuild src/index.ts --bundle --format=iife --global-name=Containo --outfile=dist/index.umd.js"
  },
  "sideEffects": false,
  "type": "module",
  "types": "dist/index.d.ts"
}
