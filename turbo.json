{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ],
      "env": [
        "ADMIN_TOKEN",
        "AIR_DIVISOR",
        "AIR_PRICE_PER_KG",
        "API_KEY_PEPPER",
        "API_URL",
        "AUTH_SECRET",
        "AUTOBOOK_FORCE_WINDOW_HOURS",
        "AUTOBOOK_HOURS_BEFORE",
        "BETTER_AUTH_SECRET",
        "BETTER_AUTH_URL",
        "BOOKING_HTTP_AUTH",
        "BOOKING_HTTP_URL",
        "COURIER_SANDBOX_API_KEY",
        "COURIER_SANDBOX_URL",
        "CRON_POOLS_ENABLED",
        "CRON_POOLS_PERIOD_SEC",
        "DATABASE_URL",
        "EMAIL_OTP_API_SECRET",
        "FACEBOOK_CLIENT_ID",
        "FACEBOOK_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "MAIL_BCC",
        "MAIL_FROM",
        "MAIL_REPLY_TO",
        "MAIL_TEST_MODE",
        "MIN_BOOK_FILL",
        "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
        "NEXT_PUBLIC_TURNSTILE_SITE_KEY",
        "POOL_BOOK_AFTER_HOURS",
        "POOL_INTRANSIT_AFTER_HOURS",
        "POOL_SEA_CAP_M3",
        "PORT",
        "PUBLIC_WEB_URL",
        "REDIS_TOKEN",
        "REDIS_URL",
        "RESEND_API_KEY",
        "RESEND_API_KEY",
        "SEA_PRICE_PER_CBM",
        "SERVICE_FEE_PER_ORDER",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "TURNSTILE_SECRET_KEY",
        "UPSTASH_REDIS_REST_TOKEN",
        "UPSTASH_REDIS_REST_URL",
        "WEBHOOK_MAX_ATTEMPTS",
        "API_BASE_URL",
        "WEB_BASE_URL"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
